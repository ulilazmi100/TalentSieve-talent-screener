{
  "name": "talentsieve-rag-talent-screener",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "start": "node src/server.js",
    "worker": "node src/worker.js",
    "dev": "nodemon --watch src --exec node src/server.js",
    "test": "jest --runInBand",
    "lint": "echo \"no lint configured\"",
    "ci": "npm run test",
    "migrate:up": "PGPASSWORD=${PGPASSWORD:-postgres} psql -h ${PGHOST:-localhost} -p ${PGPORT:-5432} -U ${PGUSER:-postgres} -d ${PGDATABASE:-talentsieve} -f migrations/up.sql",
    "migrate:down": "PGPASSWORD=${PGPASSWORD:-postgres} psql -h ${PGHOST:-localhost} -p ${PGPORT:-5432} -U ${PGUSER:-postgres} -d ${PGDATABASE:-talentsieve} -f migrations/down.sql"
  },
  "dependencies": {
    "@qdrant/js-client-rest": "^1.11.0",
    "axios": "^1.7.0",
    "bullmq": "^5.61.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.0",
    "form-data": "^4.0.0",
    "ioredis": "^5.3.2",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "undici": "^7.16.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "ajv": "^8.17.1",
    "jest": "^29.7.0",
    "nock": "^13.5.0",
    "supertest": "^7.0.0",
    "pdfkit": "^0.14.0"
  },
  "engines": {
    "node": ">=18"
  }
}
