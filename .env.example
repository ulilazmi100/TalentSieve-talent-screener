PORT=3000
NODE_ENV=development

# Gemini
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-2.5-flash
GEMINI_EMBED_MODEL=gemini-embedding-001

# Redis / BullMQ
REDIS_URL=redis://redis:6379

# Postgres
# Note: docker-compose creates DB 'talentsieve' by default
DATABASE_URL=postgresql://postgres:postgres@postgres:5432/talentsieve

# Qdrant
QDRANT_URL=http://qdrant:6333

# Defaults
DEFAULT_PROVIDER=gemini
DEMO_MODE=true

# Qdrant vector size (verify with scripts/test_embed.js)
QDRANT_VECTOR_SIZE=3072
